{
  "name": "nahmii-cli",
  "version": "1.3.0",
  "description": "CLI interface for using the nahmii by hubii protocol",
  "main": "index.js",
  "scripts": {
    "test": "nyc --all mocha \"src/**/*.spec.js\"",
    "acceptance:test": "mocha --timeout 999999 \"tests/**/*.spec.js\"",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "codeclimate": "npx gulp run-codeclimate"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hubiinetwork/nahmii-cli.git"
  },
  "keywords": [
    "hubii",
    "nahmii",
    "striim",
    "cli",
    "api",
    "micropayments",
    "trading",
    "dex",
    "ethereum",
    "layer2"
  ],
  "author": "Morten Fjeldstad <morten@hubii.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/hubiinetwork/nahmii-cli/issues"
  },
  "homepage": "https://github.com/hubiinetwork/nahmii-cli#readme",
  "bin": {
    "nahmii": "src/main.js"
  },
  "dependencies": {
    "bson": "1.1.1",
    "ethers": "^4.0.37",
    "js-yaml": "^3.13.1",
    "nahmii-contract-abstractions": "2.2.0",
    "nahmii-contract-abstractions-ropsten": "3.2.2",
    "nahmii-ethereum-address": "^2.1.0",
    "nahmii-sdk": "4.4.1",
    "node-yaml": "^4.0.1",
    "ora": "^3.4.0",
    "readline-sync": "^1.4.10",
    "superagent": "^5.0.2",
    "web3-utils": "^1.2.1",
    "yargs": "^13.3.0"
  },
  "devDependencies": {
    "@hubiinetwork/eslint-config": "github:hubiinetwork/eslint-config",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.4.0",
    "gulp": "^4.0.2",
    "gulp-cli": "^2.2.0",
    "mocha": "^7.1.1",
    "nock": "^12.0.3",
    "nyc": "^15.0.0",
    "pre-commit": "^1.2.2",
    "proxyquire": "^2.1.3",
    "shelljs": "^0.8.3",
    "sinon": "^9.0.1",
    "sinon-chai": "^3.3.0"
  }
}
